{
    "version":2,
    "builds":[
        {"src":"src/main"},
        {"use":"@vercel/node"}
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"src/main.ts",
            "methods":[
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}